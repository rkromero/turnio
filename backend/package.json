{
  "name": "turnio-backend",
  "version": "1.0.1",
  "description": "Backend API para TurnIO - Plataforma SaaS de Gesti√≥n de Turnos",
  "main": "src/index.js",
  "scripts": {
    "dev": "nodemon src/index.js",
    "start": "npm run db:setup && npm run db:generate && node src/index.js",
    "build": "echo 'No build needed for Node.js'",
    "db:migrate": "prisma migrate dev",
    "db:deploy": "prisma migrate deploy && prisma generate",
    "db:setup": "prisma db push && prisma generate",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "test": "jest --verbose --runInBand",
    "test:watch": "jest --watch --verbose",
    "test:payment": "jest payment-validation.test.js --verbose",
    "railway-fix": "node railway-fix.js"
  },
  "keywords": [
    "saas",
    "turnos",
    "appointments",
    "booking"
  ],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "dependencies": {
    "@prisma/client": "^6.10.1",
    "axios": "^1.10.0",
    "bcryptjs": "^3.0.2",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^4.21.2",
    "express-rate-limit": "^7.5.0",
    "express-validator": "^7.2.1",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "mercadopago": "^2.7.0",
    "morgan": "^1.10.0",
    "node-fetch": "^3.3.2",
    "nodemailer": "^6.10.1",
    "prisma": "^6.9.0",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "nodemon": "^3.1.10",
    "jest": "^29.7.0",
    "supertest": "^7.0.0"
  },
  "jest": {
    "testEnvironment": "node",
    "collectCoverage": false,
    "testTimeout": 30000,
    "setupFilesAfterEnv": ["<rootDir>/test/setup.js"]
  }
}
